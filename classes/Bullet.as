package  {		import flash.display.MovieClip;			public class Bullet extends Hazard {				public var dx:Number; 		public var dy:Number;				public function Bullet(new_x:Number, new_y:Number, new_dx:Number, new_dy:Number) {			// constructor code			x = new_x; 			y = new_y;			dx = new_dx;			dy = new_dy;		}				public function Move()		{			x += dx;			y += dy;		}	}	}