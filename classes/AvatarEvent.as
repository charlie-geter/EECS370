package {	import flash.events.Event;	public class AvatarEvent extends Event	{		public static const DEAD:String = "dead";		public static const GOAL_REACHED:String = "goal_reached";		public static const RESTART:String = "restart";		public static const QUIT:String = "quit";		public static const JETPACK:String = "jetpack";		public static const TIME_OVER:String = "time_over";				public var levelnum:Number;				public function AvatarEvent(type:String, nextLevel:Number = 0)		{			super(type);			levelnum = nextLevel;		}	}}